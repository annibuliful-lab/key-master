  "prisma": {
    "seed": "ts-node prisma/seed/index.ts"
  },
  "scripts": {
    "start": "nx serve",
    "dev:backend": "nx run-many --target=serve --projects=user-backend,auth-backend,project-backend,key-management-backend,user-activity-backend,gateway-backend",
    "build": "nx run-many --target=build --projects=user-backend,auth-backend,project-backend,key-management-backend,user-activity-backend,gateway-backend --parallel",
    "test": "yarn run test:backend && yarn run utils:test && yarn run graphql:test",
    "graphql:test": "nx test graphql",
    "test:backend": "concurrently \"yarn:*-backend:test\"",
    "all-services:dev": "concurrently \"yarn:*-backend:dev\"",
    "user-backend:test": "nx test user-backend",
    "auth-backend:test": "nx test auth-backend",
    "project-backend:test": "nx test project-backend",
    "key-management-backend:test": "nx test key-management-backend",
    "user-activity-backend:test": "nx test user-activity-backend",
    "key-management-backend:dev": "nx serve key-management-backend",
    "utils:test": "nx test utils",
    "user-backend:dev": "nx serve user-backend",
    "auth-backend:dev": "nx serve auth-backend",
    "project-backend:dev": "nx serve project-backend",
    "user-activity-backend:dev": "nx serve user-activity-backend",
    "gateway-backend:dev": "nx serve gateway-backend",
    "dashboard:dev": "nx serve dashboard",
    "db:migrate": "prisma migrate dev",
    "db:seed": "prisma db seed",
    "db:generate": "prisma generate",
    "db-mongo:generate": "prisma generate --schema prisma-mongo/schema.prisma",
    "db-mongo:seed": "ts-node prisma-mongo/seed/index.ts",
    "db-all:generate": "yarn run db:generate && yarn db-mongo:generate",
    "db-all:seed": "yarn run db:seed && yarn run db-mongo:seed",
    "genql": "ts-node tools/bin/genql.ts",
    "codegen:backend": "ts-node tools/bin/codegen.ts",
    "codegen:frontend": "graphql-codegen --config codegen-frontend.yml",
    "codegen": "yarn run codegen:backend & yarn run codegen:frontend",
    "generate": "yarn codegen:backend && yarn db:generate && yarn db-mongo:generate",
    "seed": "yarn db:seed && yarn db-mongo:seed"
  },
